generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Message {
  id      Int    @id @default(autoincrement())
  content String
  stars   Int    @default(1)
}

model Proyects {
  id   Int    @id @default(autoincrement())
  name String
}
